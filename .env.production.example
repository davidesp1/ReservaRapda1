# Configuração de Produção para Vercel
# Copie este arquivo e configure as variáveis na dashboard da Vercel

# Base de Dados (PostgreSQL)
DATABASE_URL=postgresql://user:password@host:port/database

# EuPago - Pagamentos
EUPAGO_API_KEY=your_eupago_api_key
EUPAGO_CLIENT_ID=your_client_id  
EUPAGO_CLIENT_SECRET=your_client_secret

# Supabase - Storage de Imagens
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Sessão e Segurança
SESSION_SECRET=your_secure_random_string_min_32_characters

# Configuração de Produção
NODE_ENV=production
FRONTEND_URL=https://your-project.vercel.app

# Opcional - Configurações Adicionais
PORT=5000
CORS_ORIGIN=https://your-project.vercel.app