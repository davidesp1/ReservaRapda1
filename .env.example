# Exemplo de configuração - Copie este arquivo para .env.local e ajuste os valores

# Supabase (Banco de Dados e Armazenamento)
DATABASE_URL=postgres://postgres:[SEU-PASSWORD]@db.[SEU-PROJETO].supabase.co:6543/postgres
SUPABASE_URL=https://[SEU-PROJETO].supabase.co
SUPABASE_KEY=[SUA-CHAVE-ANON-KEY]

# Configuração do Servidor
PORT=5000
NODE_ENV=development
SESSION_SECRET=your-session-secret-key-here

# Integração de Pagamentos
# No modo desenvolvimento, use "demo-key-for-development"
EUPAGO_API_KEY=demo-key-for-development

# Configurações opcionais (desenvolvimento)
# DEBUG=true
# MAIL_HOST=smtp.example.com
# MAIL_PORT=587
# MAIL_USER=user@example.com
# MAIL_PASS=your-mail-password